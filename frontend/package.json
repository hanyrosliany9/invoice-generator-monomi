{
  "name": "invoice-generator-frontend",
  "version": "1.0.0",
  "description": "Monomi Finance - Financial Management Platform - React Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000 --clearScreen false",
    "build": "NODE_ENV=production vite build",
    "build:with-typecheck": "tsc --project tsconfig.production.json && NODE_ENV=production vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:edge-cases": "vitest --run src/utils/test-helpers.test.ts",
    "validate": "npm run type-check && npm run lint && npm run test",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc --noEmit"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
